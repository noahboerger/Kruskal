import java.util.*;
public class KauskalTest{
public static void main(String[] args){
UndiGraph g= UndiGraphIO.readUndiGraph();
UndiGraphIO.printUndiGraph(g);
kruskal.kruskal(g);
System.out.print("Der minimale Spannbaum ");
System.out.println("besteht aus folgenden Kanten :\n ");
for(UndiEdge e : g.edges() ) {
if (e.status )
System.out.println("(" +e.left.name+","+e.right.name+") " +e.cost);
}
}
}

